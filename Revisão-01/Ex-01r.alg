algoritmo "Multiplicação do Número"

var
n1, n2, n3, res : real 

inicio
Escreval("Digite o primeiro número")
leia (n1)
Escreval("Digite o segundo numero")
leia (n2)
Escreval ("Digite o terceiro número")
leia (n3)
res <- (n1 * n2 * n3)
Escreval ("O resultado da multiplicação dos números é ",res)

fimalgoritmo